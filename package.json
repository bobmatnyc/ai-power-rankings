{
  "name": "ai-power-rankings",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/dev-server.js",
    "dev:default": "next dev --turbopack",
    "build": "next build",
    "build:with-types": "npm run type-check && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "build:types": "tsc --noEmit",
    "ci:local": "npm run lint && npm run type-check && npm run format:check && npm run test",
    "pre-deploy": "npm run ci:local",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "db:generate-types": "supabase gen types typescript --local > src/types/supabase.ts",
    "db:reset": "supabase db reset",
    "db:seed": "tsx scripts/seed-database.ts",
    "db:migrate:tools-json": "tsx scripts/apply-tools-json-migration.ts",
    "db:migrate:tools-json-safe": "tsx scripts/apply-tools-json-migration-safe.ts",
    "collect:github": "tsx scripts/collect-github-data.ts",
    "generate:rankings": "tsx scripts/generate-rankings.ts",
    "extract-metrics": "tsx scripts/extract-metrics-from-article.ts",
    "export-by-date": "tsx scripts/export-by-date.ts",
    "export-showcase": "tsx scripts/export-showcase.ts",
    "check:tools-schema": "tsx scripts/check-tools-schema.ts",
    "prepare": "husky",
    "version:patch": "npm version patch && echo $(npm pkg get version | tr -d '\"') > VERSION",
    "version:minor": "npm version minor && echo $(npm pkg get version | tr -d '\"') > VERSION",
    "version:major": "npm version major && echo $(npm pkg get version | tr -d '\"') > VERSION",
    "i18n:crawl": "node scripts/crawl-missing-translations.js",
    "i18n:debug": "curl -s http://localhost:3000/api/debug/missing-translations | jq",
    "i18n:sync": "tsx src/lib/i18n/scripts/sync-translations.ts",
    "i18n:summary": "tsx src/lib/i18n/scripts/translation-summary.ts",
    "i18n:check": "node src/lib/i18n/scripts/check-translations.mjs",
    "seo:submit-sitemap": "tsx src/lib/seo/submit-sitemap.ts",
    "check-deployment": "tsx src/scripts/check-vercel-deployment.ts",
    "fix-deployment": "tsx src/scripts/fix-deployment-errors.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.0",
    "@marsidev/react-turnstile": "^1.1.0",
    "@next/third-parties": "^15.3.4",
    "@octokit/rest": "^22.0.0",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.0",
    "@types/nodemailer": "^6.4.17",
    "@vercel/analytics": "^1.5.0",
    "@vercel/og": "^0.6.8",
    "@vercel/speed-insights": "^1.2.0",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "axios": "^1.9.0",
    "cheerio": "^1.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csv-parser": "^3.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "googleapis": "^150.0.1",
    "lucide-react": "^0.513.0",
    "next": "15.3.3",
    "next-auth": "5.0.0-beta.28",
    "nodemailer": "^6.9.15",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.57.0",
    "reading-time": "^1.5.0",
    "recharts": "^2.15.3",
    "resend": "^4.5.2",
    "schema-dts": "^1.1.5",
    "server-only": "^0.0.1",
    "supabase": "^2.24.3",
    "tailwind-merge": "^3.3.0",
    "zod": "^3.25.56",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.19.0",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.5.1",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "eslint-config-prettier": "^10.1.5",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,scss}": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@8.15.4+sha512.0bd3a9be9eb0e9a692676deec00a303ba218ba279d99241475616b398dbaeedd11146f92c2843458f557b1d127e09d4c171e105bdcd6b61002b39685a8016b9e"
}
