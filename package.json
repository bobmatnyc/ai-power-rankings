    "test:api:companies": "tsx scripts/test-companies-api.ts",
    "test:api:tools": "tsx scripts/test-tools-api.ts",
    "test:api:rankings": "tsx scripts/test-rankings-api.ts",
    "test:api:all": "npm run test:api:news && npm run test:api:companies && npm run test:api:tools && npm run test:api:rankings",
    "test:i18n": "tsx scripts/test-i18n-imports.ts",
    "test:rate-limit": "tsx scripts/test-rate-limiting.ts",
    "perf:audit": "node scripts/performance-audit.js",
