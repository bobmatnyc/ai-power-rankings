    "backup:restore:latest": "tsx src/scripts/restore-json-data.ts --latest",
    "validate:all": "tsx src/scripts/validate-json-data.ts",
    "health:check": "npm run validate:all",
    "optimize:json": "tsx src/scripts/optimize-json-performance.ts",
    "perf:analyze": "tsx src/scripts/optimize-json-performance.ts",
    "cache:stats": "curl -s http://localhost:3000/api/cache/stats | jq",
    "json:migrate:news": "tsx scripts/json-migration/migrate-news-data.ts",
    "json:migrate:companies": "tsx scripts/json-migration/migrate-companies-data.ts",
    "json:validate": "tsx scripts/json-migration/validate-migration.ts",
    "json:rollback": "tsx scripts/json-migration/rollback-migration.ts rollback",
    "json:rollback:list": "tsx scripts/json-migration/rollback-migration.ts list",
