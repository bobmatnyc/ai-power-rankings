{
  "name": "AI Power Rankings",
  "version": "1.0.0",
  "description": "Access AI Power Rankings data",
  "authentication": "none",
  "base_url": "https://aipowerranking.com/api/mcp",
  "endpoints": [
    {
      "path": "/rankings",
      "method": "GET",
      "description": "Get current rankings"
    },
    {
      "path": "/tools/{id}",
      "method": "GET", 
      "description": "Get tool details"
    },
    {
      "path": "/search",
      "method": "POST",
      "description": "Search tools"
    }
  ]
}