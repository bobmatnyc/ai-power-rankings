{
  "name": "ai-power-rankings-mcp",
  "version": "1.0.0",
  "description": "MCP server for AI Power Rankings database",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "ai-power-rankings-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/simple-server.js",
    "dev": "tsx src/simple-server.ts",
    "dev:mcp": "tsx src/index.ts",
    "dev:web": "tsx src/web-gateway.ts",
    "dev:ngrok": "concurrently \"npm run dev:web\" \"ngrok http 3001 --domain=1mbot.ngrok.app\""
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@supabase/supabase-js": "^2.38.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "concurrently": "^8.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
